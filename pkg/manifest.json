{
    "manifest_version": 2,
    "name": "wino",
    "version": "1.3",
    "description": "Painful RSS reader WebExtension, almost written by rust.",
    "browser_action": {
        "default_icon": "beasts-32.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
    "optional_permissions": [
        "<all_urls>"
    ]
}
